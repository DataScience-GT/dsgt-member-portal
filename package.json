{
  "name": "dsgt-member-portal",
  "version": "1.0.0",
  "description": "the member portal for dsgt 202",
  "main": "index.js",
  "scripts": {
    "heroku-prebuild": "cd client && npm install && npm run build",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "db:migrate": "cd dist && knex migrate:latest",
    "db:seed": "cd dist && knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnRamberger/dsgt-member-portal.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JohnRamberger/dsgt-member-portal/issues"
  },
  "homepage": "https://github.com/JohnRamberger/dsgt-member-portal#readme",
  "dependencies": {
    "concurrently": "^7.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "knex": "^2.1.0",
    "md5": "^2.3.0",
    "nodemailer": "^6.7.7",
    "nodemon": "^2.0.19",
    "pg": "^8.7.3",
    "qrcode": "^1.5.1",
    "request-ip": "^3.3.0",
    "stripe": "^9.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.3",
    "ts-node": "^8.10.2",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "16.11.1"
  }
}
